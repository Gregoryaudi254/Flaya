{
  "expo": {
    "name": "flaya",
    "slug": "flayaco",
    "version": "1",
    "orientation": "portrait",
    "icon": "./assets/images/flareapp.png",
    "scheme": "flaya",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/flaya_main.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flayasapp.flayaco",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "targetSdkVersion": 35,
      "notification": {
        "icon": "./assets/images/flareapp.png",
        "color": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCxuU-tNQrj6WbpsHRooQN0vDEyOr1nb8E"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/flareapp.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.flayasapp.flayaco",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      "react-native-compressor"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3fbda417-b2c3-45b4-b5b7-bba63ccad7fa"
      }
    },
    "owner": "flayasapp"
  }
}
